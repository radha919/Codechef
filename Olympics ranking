#include <iostream>
using namespace std;

int main() {
	int t;
	cin>>t;
	while(t--)
	{
	    int x1,x2,x3,y1,y2,y3;
	    cin>>x1>>x2>>x3>>y1>>y2>>y3;
	    int a,b;
	    a=x1+x2+x3;
	    b=y1+y2+y3;
	    if(a>b)
	    cout<<"1"<<endl;
	    else
	    cout<<"2"<<endl;
	    
	}// your code goes here
	return 0;
}
